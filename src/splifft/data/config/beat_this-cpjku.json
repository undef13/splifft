{
    "identifier": "beat_this-cpjku-final",
    "model_type": "beat_this",
    "model": {
        "chunk_size": 661500,
        "output_stem_names": [
            "beat",
            "downbeat"
        ]
    },
    "audio_io": {
        "target_sample_rate": 22050,
        "force_channels": 1
    },
    "transform": {
        "kind": "mel",
        "n_fft": 1024,
        "hop_length": 441,
        "n_mels": 128,
        "sample_rate": 22050,
        "f_min": 30.0,
        "f_max": 11000.0
    },
    "inference": {
        "batch_size": 1
    },
    "normalization": {
        "enabled": false
    },
    "sequence_chunking": {
        "trim_margin": 6,
        "overlap_mode": "keep_first",
        "avoid_short_end": true
    },
    "output": {
        "file_format": "npy"
    }
}